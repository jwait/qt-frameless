background:#096dd9;
}

QPushButton[type="important"]:checked,
QToolButton[type="important"]:checked{
border:1px solid #1890FF;
background:#1890FF;
}

QPushButton[type="important"]:disabled,
QToolButton[type="important"]:disabled{
color:#999999;
border:1px solid #79798F;
background:#464655;
}

/**********************
menu bar pushbutton
***********************/
