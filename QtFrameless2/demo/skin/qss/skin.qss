
QPalette{background:#464655;}*{outline:0px;color:#FFFFFF;}

/*window icon*/
QLabel#label_logo,QLabel[type="logo"]{
        image: url(:/skin/dark/logo.png);
}

/*title bar widget*/
QLabel[type="title"][size="25"]{
font-size:25px;
}

/*大标题*/
QLabel[type="title"]{
font-size:15px;
font-weight:bold;
}

/*小标题*/
QLabel[type="sub_title"]{
font-size:13px;
font-weight:bold;
}

QLabel[type="emphasize"]{
font-size:13px;
}

QLabel[type="text"]{
font-size:12px;
}

QLabel[type="toast_text"]{
font-size:15px;
color:#FFFFFF;
}

QLabel[type="loading_text"]{
font-size:15px;
color:#FFFFFF;
}

QLabel[type="loading_progressNum"]{
font-size:15px;
color:#1890FF;
}

QWidget[type="title_bar"]
{
padding:5px;
color:#000000;
background:#464655;
}

/*子对话框，客户区相比标题栏颜色加深child_with_nav*/
QWidget[window="child_with_nav"] QWidget[type="title_bar"]
{
padding:5px;
color:#000000;
background:#464655;
}

/*子对话框，标题栏跟客户区同色*/
QWidget[window="child"] QWidget[type="title_bar"]
{
padding:5px;
color:#000000;
background:#464655;

border-radius:0px;
border: 1px solid #79798F;
border-left: none;
border-top: none;
border-right: none;
}

QWidget[window="prompt"] QWidget[type="title_bar"]
{
padding:5px;
color:#000000;
background:#4E5C6D;

border-radius:0px;
border: 1px solid #79798F;
border-left: none;
border-top: none;
border-right: none;
}

QWidget[type="toast"]
{
background-color:#3A3A4A;
border-radius:10px;
}

QWidget[type="startup"]
{
background-color:#464655;
border-radius:10px;
}

QWidget[type="loading"]
{
background-color:#4E5C6D;
border-radius:10px;
}

/*title bar widget bottom border*/
QWidget[type="title_bar"][border=true]
{
border-radius:0px;
border: 1px solid #79798F;
border-left: none;
border-top: none;
border-right: none;
}

/*menu bar widget*/
QWidget[type="menu_bar"]
{
padding:5px;
color:#000000;
background:#464655;
}
/*menu bar widget bottom border*/
QWidget[type="menu_bar"][border=true]
{
border-radius:0px;
border: 1px solid #3A3A4B;
border-left: none;
border-top: none;
border-right: none;
}

/*tool bar widget*/
.QWidget[type="tool_bar"]
{
padding:5px;
color:#000000;
background:#464655;
}
/*tool bar widget bottom border*/
.QWidget[type="tool_bar"][border=true]
{
border-radius:0px;
border: 1px solid #3A3A4B;
border-left: none;
border-right: none;
}

/*client widget*/
QWidget[type="client"]
{
background:#464655;
}

QWidget[window="child_with_nav"] QWidget[type="client"],
QWidget[window="child_with_nav"] QWidget[type="client_with_nav"]
{
background:#3A3A4A;
}

QWidget[window="child"] QWidget[type="client"]
{
background:#464655;
}

QWidget[window="prompt"] QWidget[type="client"]
{
background:#4E5C6D;
}

QWidget[type="content"]
{
background:#464655;
}

QFrame[type="sub_pane"]
{
background:#464655;
border: 1px solid #4E5C6D;
}

/*status bar widget*/
QWidget[type="status_bar"]
{
padding:5px;
color:#FFFFFF;
background:#464655;
border-radius:0px;
border: 1px solid #3A3A4B;
border-left: none;
border-bottom: none;
border-right: none;
}

/*status bar widget top border*/
QWidget[type="status_bar"][border=true]
{
border-radius:0px;
border: 1px solid #3A3A4B;
border-left: none;
border-bottom: none;
border-right: none;
}

QWidget[type="status_bar"] QLabel
{
color:#FFFFFF;
font-size:13px;
}

QWidget[type="status_bar"] QLabel:hover
{
color:#1890FF;
}

StatusBarTextFrame QLabel[level="normal"]
{
color:#389E0D;
}
StatusBarTextFrame QLabel[level="warnning"]
{
color:#FF6600;
}
StatusBarTextFrame[hover=true] QLabel
{
color:#1890FF;
}

/**********************
line
***********************/
QFrame[type="ver_line"]
{
border:none;
background-color:#4E5C6D;
max-width:1px;
min-width:1px;
}

QFrame[type="hor_line"]
{
border:none;
background-color:#4E5C6D;
max-height:1px;
min-height:1px;
}

/**********************
edit input widget
***********************/
QLineEdit,QTextEdit,QPlainTextEdit,
QSpinBox,QDoubleSpinBox,
QComboBox,
QDateEdit,QTimeEdit,QDateTimeEdit{
border:1px solid #79798F;
border-radius:4px;
padding-left:5px;
color:#FFFFFF;
background:#464655;
selection-background-color:#1890FF;
selection-color:#FFFFFF;
min-height:22px;
}

QLineEdit:focus,QTextEdit:focus,QPlainTextEdit:focus,
QSpinBox:focus,QDoubleSpinBox:focus,
QComboBox:focus,
QDateEdit:focus,QTimeEdit:focus,QDateTimeEdit:focus,
QLineEdit:hover,
QTextEdit:hover,QPlainTextEdit:hover,QSpinBox:hover,QDoubleSpinBox:hover,QComboBox:hover,QDateEdit:hover,QTimeEdit:hover,QDateTimeEdit:hover{
border:1px solid #1890FF;
}

QLineEdit:hover,QTextEdit:hover,QPlainTextEdit:hover,
QSpinBox:hover,QDoubleSpinBox:hover,
QComboBox:hover,
QDateEdit:hover,QTimeEdit:hover,QDateTimeEdit:hover{
border:1px solid #1890FF;
}

QLineEdit[echoMode="2"]{
lineedit-password-character:9679;
}

QLineEdit:disabled,QTextEdit:disabled,QPlainTextEdit:disabled,
QSpinBox:disabled,QDoubleSpinBox:disabled,
QComboBox:disabled,
QDateEdit:disabled,QTimeEdit:disabled,QDateTimeEdit:disabled{
background: #4E5C6D;
color: #999999;
}

QLineEdit:read-only,QTextEdit:read-only,QPlainTextEdit:read-only,
QSpinBox:read-only,QDoubleSpinBox:read-only,
QDateEdit:read-only,QTimeEdit:read-only,QDateTimeEdit:read-only{
background: #4E5C6D;
color: #FFFFFF;
}

QLineEdit[type="CalendarSelector"]:read-only{
background:#464655;
color: #FFFFFF;
}

QLineEdit[type="CalendarSelector"][calendarPopup=true]{
border:1px solid #79798F;
border-radius:4px;
padding-left:5px;
color:#FFFFFF;
background:#464655;
selection-background-color:#1890FF;
selection-color:#FFFFFF;
min-height:22px;
/*border-bottom-left-radius:0px;
border-bottom-right-radius:0px;*/
}

QLineEdit[type="CalendarSelector"][calendarPopup=true]:focus,
QLineEdit[type="CalendarSelector"][calendarPopup=true]:hover{
border:1px solid #79798F;
}

QTimeEdit::up-button,
QDateEdit::up-button,
QDateTimeEdit::up-button,
QDoubleSpinBox::up-button,
QSpinBox::up-button{
image:url(:/skin/dark/other/array_up.png);
width:10px;
height:10px;
padding:3px 3px 3px 0px;
}

QTimeEdit::down-button,
QDateEdit::down-button,
QDateTimeEdit::down-button,
QDoubleSpinBox::down-button,
QSpinBox::down-button{
image:url(:/skin/dark/other/array_down.png);
width:10px;
height:10px;
padding:3px 3px 3px 0px;
}

QTimeEdit::up-button:pressed,
QDateEdit::up-button:pressed,
QDateTimeEdit::up-button:pressed,
QDoubleSpinBox::up-button:pressed,
QSpinBox::up-button:pressed{
top:-1px;
}

QTimeEdit::down-button:pressed,
QDateEdit::down-button:pressed,
QDateTimeEdit::down-button:pressed,
QDoubleSpinBox::down-button:pressed,
QSpinBox::down-button:pressed,
QSpinBox::down-button:pressed{
bottom:-1px;
}

QComboBox::down-arrow,
QDateEdit[calendarPopup="true"]::down-arrow,
QTimeEdit[calendarPopup="true"]::down-arrow,
QDateTimeEdit[calendarPopup="true"]::down-arrow{
image:url(:/skin/dark/other/array_down.png);
width:10px;
height:10px;
right:2px;
}

QComboBox::drop-down,
QDateEdit::drop-down,
QTimeEdit::drop-down,
QDateTimeEdit::drop-down{
subcontrol-origin:padding;
subcontrol-position:top right;
width:15px;
border-left-width:0px;
border-left-style:solid;
border-top-right-radius:3px;
border-bottom-right-radius:3px;
border-left-color:#79798F;
}

QComboBox::drop-down:on{
top:1px;
}

.QGroupBox{
border:1px solid #4E5C6D;
border-radius:5px;
margin-top:3ex;
font-size:13px;
font-weight:bold;
}

.QGroupBox::title{
subcontrol-origin:margin;
position:relative;
left:10px;
}

/**********************
normal button
***********************/

.QPushButton{
border:1px solid #79798F;
border-radius:4px;
color:#FFFFFF;
font-size:12px;
padding-left:10px;
padding-right:10px;
background:transparent;/*#464655;*/
/*min-height:24px;*/
}

QWidget#CalendarBottomWidget QPushButton,
QWidget#CalendarBottomWidget QToolButton{
min-height:22px;
}

.QPushButton:hover{
border:1px solid #1890FF;
background:#464655;
}

.QPushButton:pressed{
border:1px solid #1890FF;
background:#3A3A4A;
}

.QPushButton:checked{
color:#1890FF;
border:1px solid #1890FF;
background:#464655;
}

.QPushButton:disabled{
color:#999999;
border:1px solid #79798F;
background:#464655;
}

.QPushButton[type="color_button"]{
border:1px solid #FFFFFF;
border-radius:0px;
}
.QPushButton[type="color_button"]:hover{
border:2px solid #000000;
}
.QPushButton[type="color_button"]:pressed{
border:2px solid #000000;
}
.QPushButton[type="color_button"]:checked{
border:2px solid #000000;
}

.QToolButton{
border:0px;
border-radius:4px;
color:#FFFFFF;
font-size:12px;
padding-left:0px;
padding-right:0px;
background:#464655;
}

.QToolButton[popup_menu=true]{
border:1px solid #79798F;
border-radius:4px;
color:#FFFFFF;
font-size:12px;
padding-left:5px;

background:#464655;
}

.QToolButton:hover{
border:1px solid #1890FF;
background:#464655;
}

.QToolButton:pressed{
border:1px solid #1890FF;
background:#3A3A4A;
}

.QToolButton:checked{
color:#1890FF;
border:1px solid #1890FF;
background:#464655;
}

.QToolButton:disabled{
color:#999999;
border:1px solid #79798F;
background:#464655;
}

QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 16px; /* make way for the popup button */
}

/* the subcontrols below are used only in the MenuButtonPopup mode */
QToolButton::menu-button {
    border: 0px solid gray;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    /* 16px width + 4px for border = 20px allocated above */
    width: 16px;
}

QToolButton::menu-arrow:open {
    top: 0px;
    left: 0px; /* shift it a bit */
}

QToolButton::menu-arrow{
image:url(:/skin/dark/other/array_down.png);
width:10px;
height:10px;
}

/**********************
important button
***********************/
QPushButton[type="important"],
QToolButton[type="important"]{
border:1px solid #1890FF;
border-radius:4px;
color:#FFFFFF;
font-size:12px;
padding-left:10px;
padding-right:10px;
