background:transparent;
}

QPushButton[type="label_button"]:hover,
QToolButton[type="label_button"]:hover{
border-style:none;
color:#1F86F6;
background:transparent;
}

QPushButton[type="label_button"]:pressed,
QToolButton[type="label_button"]:pressed{
border-style:none;
color:#096dd9;
background:transparent;
}

QPushButton[type="label_button"]:disabled,
QToolButton[type="label_button"]:disabled{
border-style:none;
color:#999999;
background:transparent;
}

QPushButton[type="label_button"][selected=true]{
color:#FFFFFF;
}

QPushButton[type="label_button"][selected=true]:hover,
QPushButton[type="label_button"][selected=true]:pressed{
color:#FFFFFF;
}

/**********************
switch button
***********************/
QPushButton[type="switch_button"][direction="left"],
QToolButton[type="switch_button"][direction="left"]{
