/*QComboBox QAbstractItemView::item {
   min-height: 30px;
}*/

QComboBox QAbstractItemView {
    selection-background-color:#1890FF;
    selection-color:#FFFFFF;
}

QComboBox QAbstractItemView {
    background-color:#4E5C6D;
    color:#FFFFFF;
}

QComboBox QAbstractItemView::item:hover {
color:#FFFFFF;
background:#1890FF;
}

QMenuBar {
        background:#464655;
}

/*垂直偏移需要配合菜单栏高度去调整*/
QMenuBar::item {
        border: 0px;
        padding: -3px 10px 0px 10px;
        background:#464655;
        /*transparent;*/
}
QMenuBar::item:enabled {
        color: #FFFFFF;
}
QMenuBar::item:!enabled {
        color: #999999;
}

QMenuBar::item:selected { /* when selected using mouse or keyboard */
    color:#FFFFFF;
    background: #4E5C6D;
}

QMenuBar::item:pressed {
    color: #FFFFFF;
    background: #3A3A4A;
}

QMenu {
        background-color:#4E5C6D;
        border: 0px;
}

QMenu[shadow=true] {
        border-radius:4px;
        margin:5px;
}

/*
height:25px
*/
QMenu::item {
        color: #FFFFFF;
        padding: 4px 20px 4px 20px;
}

QMenu[itemHeight="30px"]::item {
        color: #FFFFFF;
        padding: 7px 20px 7px 20px;
}

QMenu::indicator {
        width: 13px;
        height: 13px;
}

QMenu[hoveChangeColor=true]::item:selected {
        color: #FFFFFF;
        background: #1890FF;
}

QMenu::item:selected {
        color: #FFFFFF;
        background: #1890FF;
}

QMenu::item:pressed {
        color: #FFFFFF;
        background: #1890FF;
}

QMenu::item:disabled {
        color: #999999;
}

QMenu::separator {
        height: 1px;
        background: #D7D7D7;
}
QMenu::icon { /* appearance of a 'checked' icon */
    position: absolute;
